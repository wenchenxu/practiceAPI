{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:up": "docker compose -f docker-compose.dev.yml up -d",
        "dev:build": "docker compose -f docker-compose.dev.yml build --no-cache",
        "dev:artisan": "docker compose -f docker-compose.dev.yml exec app php artisan",
        "dev:logs": "docker compose -f docker-compose.dev.yml logs -f app"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.2.0",
        "tailwindcss": "^3.4.17",
        "vite": "^6.2.4"
    }
}
